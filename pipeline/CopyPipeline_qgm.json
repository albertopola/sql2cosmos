							{
								"source": {
									"name": "FAX",
									"type": "String"
								},
								"sink": {
									"path": "$['fax']"
								}
							},
							{
								"source": {
									"name": "email",
									"type": "String"
								},
								"sink": {
									"path": "$['email']"
								}
							},
							{
								"source": {
									"name": "CodAnacf",
									"type": "Int32"
								},
								"sink": {
									"path": "$['codanacf']"
								}
							},
							{
								"source": {
									"name": "Fornitore",
									"type": "Boolean"
								},
								"sink": {
									"path": "$['fornitore']"
								}
							},
							{
								"source": {
									"name": "Cliente",
									"type": "Boolean"
								},
								"sink": {
									"path": "$['cliente']"
								}
							},
							{
								"source": {
									"name": "Ente",
									"type": "Boolean"
								},
								"sink": {
									"path": "$['ente']"
								}
							}
						]
					}
				},
				"inputs": [
					{
						"referenceName": "SourceDataset_qgm",
