							{
								"source": {
									"name": "Citta",
									"type": "String"
								},
								"sink": {
									"path": "$['city']"
								}
							},
							{
								"source": {
									"name": "Provincia",
									"type": "String"
								},
								"sink": {
									"path": "$['region']"
