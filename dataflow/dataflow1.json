{
	"name": "dataflow1",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "SourceDataset_qgm",
						"type": "DatasetReference"
					},
					"name": "source1"
				}
			],
			"sinks": [],
			"transformations": [
				{
					"name": "select1"
				},
				{
					"name": "derivedColumn1"
				}
			],
			"scriptLines": [
				"source(output(",
				"          IdAnagrafica as integer,",
				"          Denominazione as string,",
				"          Indirizzo as string,",
				"          CAP as string,",
				"          Citta as string,",
				"          Provincia as string,",
				"          CF as string,",
				"          PIVA as string,",
				"          Tel as string,",
				"          FAX as string,",
				"          email as string,",
				"          CodAnacf as integer,",
				"          Fornitore as boolean,",
				"          Cliente as boolean,",
				"          Ente as boolean",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     isolationLevel: 'READ_UNCOMMITTED',",
				"     format: 'table') ~> source1",
				"source1 select(mapColumn(",
				"          IdAnagrafica,",
				"          Denominazione,",
				"          Indirizzo,",
				"          CAP,",
				"          Citta,",
				"          Provincia,",
				"          CF,",
				"          PIVA,",
				"          Tel,",
				"          FAX,",
				"          email,",
				"          CodAnacf,",
				"          Fornitore,",
				"          Cliente,",
				"          Ente,",
				"          each(match(Id),",
				"               id = $$)",
				"     ),",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> select1",
				"select1 derive(id = iif(isNull(id), toString(newGuid()), id)) ~> derivedColumn1"
			]
		}
	}
}